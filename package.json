{
  "name": "svelte-multiselect",
  "description": "Svelte multi-select component",
  "author": "Janosh Riebesell <janosh.riebesell@gmail.com>",
  "homepage": "https://svelte-multiselect.netlify.app",
  "repository": "https://github.com/janosh/svelte-multiselect",
  "license": "MIT",
  "version": "1.1.12",
  "type": "module",
  "svelte": "MultiSelect.svelte",
  "bugs": {
    "url": "https://github.com/janosh/svelte-multiselect/issues"
  },
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "package": "svelte-kit package",
    "serve": "yarn build && yarn preview",
    "prebuild": "cp readme.md src/docs.svx && sed -i.bak '1,/^<!-- remove above in docs -->$/d' src/docs.svx && rm src/docs.svx.bak",
    "predev": "yarn prebuild",
    "check": "svelte-check --ignore package"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^1.0.0-next.20",
    "@sveltejs/kit": "^1.0.0-next.180",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "hastscript": "^7.0.2",
    "mdsvex": "^0.9.8",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "rehype-autolink-headings": "^6.1.0",
    "rehype-slug": "^5.0.0",
    "svelte": "^3.43.1",
    "svelte-check": "^2.2.7",
    "svelte-preprocess": "^4.9.8",
    "svelte-toc": "^0.1.8",
    "svelte2tsx": "^0.4.7",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "vite": "^2.6.7"
  },
  "keywords": [
    "svelte",
    "multi-select",
    "single-select",
    "form",
    "input"
  ],
  "publishConfig": {
    "access": "public"
  }
}
